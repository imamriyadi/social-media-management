<div class="container-fluid">
    <div class="row">
        <div class="col-3">
            <div class="list-group" id="list-tab" role="tablist">
                <a class="list-group-item list-group-item-action active" id="list-home-list" data-toggle="list"
                   href="#list-home" role="tab" aria-controls="home">Home</a>
                <a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list"
                   href="#list-flow" role="tab" aria-controls="profile">Flow</a>
                <a class="list-group-item list-group-item-action" id="list-messages-list" data-toggle="list"
                   href="#list-module" role="tab" aria-controls="messages">Module</a>
                <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list"
                   href="#list-settings" role="tab" aria-controls="settings">Setting</a>
            </div>
            <br>
            <div id="card-info-device" class="bg-white">
                <div class="card">
                    <div class="card-header bg-primary text-white">Account Details</div>
                    <div class="card-body">
                        <p class="card-text"><i class="fa fa-id-badge"></i> User
                            name : Oracel</p>
                        <p class="card-text"><i class="fa fa-phone-square"></i>
                            My number : 6285933800315</p>
                        <p class="card-text"><i class="fa fa-laptop"></i>
                            Platform : smbi</p>
                        <p class="card-text">
                            <i class="fab fa-whatsapp-square"></i> WhatsApp version: 2.20.121.4
                        </p>
                    </div>
                </div>
            </div>

            <div class="row" id="placeholder">
                <div class="col-md-12">
                    <div class="ph-item">
                        <div class="ph-col-12">
                            <div class="ph-picture"></div>
                            <div class="ph-row">
                                <div class="ph-col-12 big"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-12" style="margin-top: 20px">
                <div id="card-barcode" class="bg-white">
                    <div class="card">
                        <div class="card-body text-center">
                            <div class="barcode">
                                <img id="qrcode" src=""
                                     style="width: 100%;height: 100%;margin-left: auto;margin-right: auto;"
                                     alt="Barcode"
                                >
                                <h6 class="card-subtitle mb-2 text-muted text-center info">Loading ...</h6>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-9">
            <div class="tab-content" id="nav-tabContent">
                <div class="tab-pane fade show active" id="list-home" role="tabpanel"
                     aria-labelledby="list-home-list">
                    <%- include('home') %>
                </div>
                <div class="tab-pane fade" id="list-flow" role="tabpanel"
                     aria-labelledby="list-profile-list">
                    <%- include('flow') %>
                </div>
                <div class="tab-pane fade" id="list-module" role="tabpanel"
                     aria-labelledby="list-messages-list">
                    <%- include('module') %>
                </div>
                <div class="tab-pane fade" id="list-settings" role="tabpanel"
                     aria-labelledby="list-settings-list">
                    <%- include('settings') %>
                </div>
            </div>
        </div>
    </div>
</div>
